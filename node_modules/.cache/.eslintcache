ed preInventory is:', result)\r\n                this.setState({...this.state, cart: []})\r\n                this.reGetPreInventory()\r\n            }).catch(err => {\r\n                console.log(\"error\", err)\r\n            })\r\n    }\r\n\r\n    componentDidMount() {\r\n        axios.get(url.url + \"/getPreInventories\")\r\n            .then(result => {\r\n                //console.log('inventory is ', result.data.Data)\r\n                var preInventoryTemp = result.data.Data.reverse()\r\n                this.setState({ ...this.state, preInventory: preInventoryTemp })\r\n                // console.log('salesTemp is ', salesTemp)\r\n            })\r\n            .catch(err => {\r\n                console.log('err occurred ', err)\r\n            })\r\n    }\r\n    componentDidUpdate() {\r\n\r\n    }\r\n    reGetPreInventory = ()=> {\r\n        axios.get(url.url + \"/getPreInventories\")\r\n            .then(result => {\r\n                //console.log('inventory is ', result.data.Data)\r\n                var preInventoryTemp = result.data.Data.reverse()\r\n                this.setState({ ...this.state, preInventory: preInventoryTemp })\r\n                // console.log('salesTemp is ', salesTemp)\r\n            })\r\n            .catch(err => {\r\n                console.log('err occurred ', err)\r\n            })\r\n    }\r\n    render() {\r\n        return <div className='row'>\r\n            <div className='col-8'>\r\n                <div className='card'>\r\n                    <div className='card-header bg-warning'>\r\n                        <h4> Current Items in preInventory State <button className='btn btn-dark btn-rounded' onClick={this.reGetPreInventory}> Refresh List</button> </h4>\r\n                        <div className=\"input-group mb-3\">\r\n                            <input type=\"text\" name='searchField' onChange={this.handleChange} className=\"form-control\" placeholder=\"Search Inventory Record By ID, Item type OR Item Model\" aria-desc