der() {\r\n        return <div className='container'>\r\n            <Tabs defaultActiveKey=\"performSale\" id=\"uncontrolled-tab-example\" className=\"mb-3\">\r\n                <Tab eventKey=\"performSale\" title=\"Perform Sale\">\r\n                    <PerformSaleComponent />\r\n                </Tab>\r\n                <Tab eventKey=\"viewSales\" title=\"View Sales\">\r\n                    <h3>second</h3>\r\n                </Tab>\r\n                <Tab eventKey=\"profile\" title=\"Profile\" >\r\n                    <h3>Your Profile</h3>\r\n                </Tab>\r\n            </Tabs>\r\n        </div>\r\n    }\r\n}","C:\\Users\\Lawrence Nkafu\\Documents\\Git Repositories\\cvs_frontend1.0\\cvs_front_end\\src\\components\\savingToInventory\\AddToInventory.component.jsx",[],"C:\\Users\\Lawrence Nkafu\\Documents\\Git Repositories\\cvs_frontend1.0\\cvs_front_end\\src\\components\\viewSale.component.jsx",["271"],"\r\nimport React from 'react'\r\nimport axios from 'axios'\r\n\r\nexport default class ViewSaleComponent extends React.Component {\r\n    constructor() {\r\n        super()\r\n        this.state = {\r\n            sales: [],\r\n            searchField: ''\r\n        }\r\n    }\r\n    handleChange = async (evt) => {\r\n        const value = evt.target.value;\r\n        const name = evt.target.name;\r\n\r\n        await this.setState({\r\n            ...this.state,\r\n            [name]: value\r\n        });\r\n    }\r\n    printSales = () => {\r\n        let sales = this.state.sales\r\n        if (this.state.searchField === '') {\r\n            return sales.map((item, index) => {\r\n                return <tr key={index}>\r\n                    <td>{item.date}</td>\r\n                    <td>{item.confirmationNumber}</td>\r\n                    <td>{item.customerName}</td>\r\n                    <td>{item.customerNumber}</td>\r\n                    <td>{item.itemsSoldSummary}</td>\r\n                    <td>{item.total}</td>\r\n                </tr>\r\n  