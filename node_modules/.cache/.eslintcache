      });\r\n    }\r\n    printSales = () => {\r\n        let sales = this.state.sales\r\n        if (this.state.searchField === '') {\r\n            return sales.map((item, index) => {\r\n                return <tr key={index}>\r\n                    <td>{item.date}</td>\r\n                    <td>{item.confirmationNumber}</td>\r\n                    <td>{item.customerName}</td>\r\n                    <td>{item.customerNumber}</td>\r\n                    <td>{item.itemsSoldSummary}</td>\r\n                    <td>{item.total}</td>\r\n                </tr>\r\n            })\r\n        } else {\r\n            return sales.map((item, index) => {\r\n                if (item.customerName.toLowerCase().includes(this.state.searchField.toLowerCase())  || item.itemsSoldSummary.toLowerCase().includes(this.state.searchField.toLowerCase()) ) {\r\n                    return <tr key={index}>\r\n                        <td>{item.date}</td>\r\n                        <td>{item.confirmationNumber}</td>\r\n                        <td>{item.customerName}</td>\r\n                        <td>{item.customerNumber}</td>\r\n                        <td>{item.itemsSoldSummary}</td>\r\n                        <td>{item.total}</td>\r\n                    </tr>\r\n                }\r\n\r\n            })\r\n        }\r\n\r\n\r\n    }\r\n\r\n    componentDidMount() {\r\n        axios.get(\"http://localhost:3012/getSales\")\r\n            .then(result => {\r\n                var salesTemp = result.data.Sales\r\n                this.setState({ ...this.state, sales: salesTemp })\r\n               // console.log('salesTemp is ', salesTemp)\r\n            })\r\n            .catch(err => {\r\n                console.log('err occurred ', err)\r\n            })\r\n    }\r\n    componentDidUpdate() {\r\n\r\n    }\r\n    render() {\r\n        return <div>\r\n            <div className='card-header bg-info'>\r\n                <h4> SALES PERFORMED</h4>\r\n                <div className=\"input-group mb-3\">\r\n                    <input type=\"text\" name='searchField' onChange={this.handleChange} className=\"form-control\" placeholder=\"Search Sale Record\" aria-describedby=\"basic-addon2\" />\r\n                    <div className=\"input-group-append\">\r\n                        <button className=\"btn btn-outline-dark\" type=\"button\" onClick={this.handleShowSearchCustomer}>Search</button>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            <div>\r\n                <table className='table table-striped table-dark table-hover'>\r\n                    <thead className='thead-primary'>\r\n                        <tr>\r\n                            <th>Date</th>\r\n                            <th>Confirmation #</th>\r\n                            <th>Customer Name</th>\r\n                            <th>Phone #</th>\r\n                            <th>Items Sold</th>\r\n                            <th>Total</th>\r\n                        </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                        {this.printSales()}\r\n                    </tbody>\r\n                </table>\r\n            </div>\r\n            <p>View sale component works</p>\r\n        </div>\r\n    }\r\n}","C:\\Users\\Lawrence Nkafu\\Documents\\Git Repositories\\cvs_frontend1.0\\cvs_front_end\\src\\components\\customer\\addCustomer.component.jsx",[],"C:\\Users\\Lawrence Nkafu\\Documents\\Git Repositories\\cvs_frontend1.0\\cvs_front_end\\src\\components\\customer\\viewCustomers.component.jsx",["304"],"\r\nimport React from 'react'\r\nimport axios from 'axios'\r\n\r\nexport default class ViewCustomersComponent extends React.Component {\r\n    constructor(props) {\r\n        super(props)\r\n        this.state = {\r\n            customers: [],\r\n            searchField: ''\r\n        }\r\n        \r\n    }\r\n    handleChange = async (evt) => {\r\n        const value = evt.target.value;\r\n        const name = evt.target.name;\r\n\r\n        await this.setState({\r\n            ...this.state,\r\n            [name]: value\r\n        });\r\n    }\r\n    printCustomers = () => {\r\n        let customers = this.state.customers\r\n      //  console.log('customers are: ', customers)\r\n        if (this.state.searchField === '') {\r\n            return customers.map((customer, index) => {\r\n                return <tr key={index}>\r\n                    <td>{customer.date}</td>\r\n                    <td>{customer.name}</td>\r\n                    <td>{customer.phoneNumber}</td>\r\n                    <td>{customer.address}</td>\r\n                    <td>{customer.email}</td>\r\n                    <td>{customer.storeName}</td>\r\n                </tr>\r\n            })\r\n        } else {\r\n            return customers.map((customer, index) => {\r\n                if (customer.name.toLowerCase().includes(this.state.searchField.toLowerCase()) ) {\r\n                    return <tr key={index}>\r\n                        <td>{customer.date}</td>\r\n                        <td>{customer.name}</td>\r\n                        <td>{customer.phoneNumber}</td>\r\n                        <td>{customer.address}</td>\r\n                        <td>{customer.email}</td>\r\n                        <td>{customer.storeName}</td>\r\n                    </tr>\r\n                }\r\n\r\n\r\n            })\r\n        }\r\n\r\n\r\n    }\r\n\r\n    componentDidMount() {\r\n        axios.get(\"http://localhost:3012/customers\")\r\n            .then(result => {\r\n                var customersTemp = result.data.Data\r\n                this.setState({ ...this.state, customers: customersTemp })\r\n               // console.log('customerTemp is ', customersTemp)\r\n            })\r\n            .catch(err => {\r\n                console.log('err occurred ', err)\r\n            })\r\n    }\r\n    componentDidUpdate() {\r\n\r\n    }\r\n    render() {\r\n        return <div>\r\n            <div className='card-header bg-info'>\r\n                <h4>  Customers</h4>\r\n                <div className=\"input-group mb-3\">\r\n                    <input type=\"text\" name='searchField' onChange={this.handleChange} className=\"form-control\" placeholder=\"Type customer's name to search\" aria-describedby=\"basic-addon2\" />\r\n                </div>\r\n            </div>\r\n            <div>\r\n                <table className='table table-striped table-dark table-hover'>\r\n                    <thead className='thead-primary'>\r\n                        <tr>\r\n                            <th>Date</th>\r\n                            <th>Customer Name</th>\r\n                            <th>Phone Number</th>\r\n                            <th>Address</th>\r\n                            <th>Email</th>\r\n                            <th>Store Name</th>\r\n                        </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                        {this.printCustomers()}\r\n                    </tbody>\r\n                </table>\r\n            </div>\r\n        </div>\r\n    }\r\n}","C:\\Users\\Lawrence Nkafu\\Documents\\Git Repositories\\cvs_frontend1.0\\cvs_front_end\\src\\components\\profile\\profile.component.jsx",[],"C:\\Users\\Lawrence Nkafu\\Documents\\Git Repositories\\cvs_frontend1.0\\cvs_front_end\\src\\components\\viewInventory\\viewInventory.component.jsx",["305"],"C:\\Users\\Lawrence Nkafu\\Documents\\Git Repositories\\cvs_frontend1.0\\cvs_front_end\\src\\components\\updateInventory\\updateInventory.component.jsx",["306"],"C:\\Users\\Lawrence Nkafu\\Documents\\cvs\\cvs_frontend1.0\\cvs_front_end\\src\\index.js",["307","308"],["309","310"],"C:\\Users\\Lawrence Nkafu\\Documents\\cvs\\cvs_frontend1.0\\cvs_front_end\\src\\App.js",[],"C:\\Users\\Lawrence Nkafu\\Documents\\cvs\\cvs_frontend1.0\\cvs_front_end\\src\\reportWebVitals.js",[],"C:\\Users\\Lawrence Nkafu\\Documents\\cvs\\cvs_frontend1.0\\cvs_front_end\\src\\testData\\modal.component.jsx",[],